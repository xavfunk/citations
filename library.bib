  volume   =  20,
  number   =  9,
  pages    = "698--711",
  month    =  sep,
  year     =  2017,
  keywords = "ayahuasca; human; magnetic resonance imaging; mindfulness;
              psychedelic after-effects;Master Thesis",
  language = "en"
}

