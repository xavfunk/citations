@ARTICLE{Carhart-Harris2018-mt,
  title    = "Psilocybin with psychological support for treatment-resistant
              depression: six-month follow-up",
  author   = "Carhart-Harris, R L and Bolstridge, M and Day, C M J and Rucker,
