@ARTICLE{Vollenweider1998-um,
  title    = "Psilocybin induces schizophrenia-like psychosis in humans via a
              serotonin-2 agonist action",
  author   = "Vollenweider, F X and Vollenweider-Scherpenhuyzen, M F and
